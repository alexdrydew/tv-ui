{
  "applicationId": "js.neutralino.zero",
  "version": "1.0.0",
  "defaultMode": "window",
  "documentRoot": "/vite-src/build/",
  "url": "/",
  "enableServer": true,
  "enableNativeAPI": true,
  "nativeAllowList": ["app.*", "filesystem.readDirectory"],
  "modes": {
    "window": {
      "title": "tv-ui",
      "width": 800,
      "height": 500,
      "minWidth": 400,
      "minHeight": 200,
      "enableInspector": true
    }
  },
  "cli": {
    "binaryName": "tv-ui",
    "extensionsPath": "/extensions/",
    "resourcesPath": "/vite-src/dist/",
    "frontendLibrary": {
      "patchFile": "/vite-src/public/index.html",
      "devUrl": "http://localhost:5173",
      "projectPath": "/vite-src/",
      "initCommand": "pnpm install",
      "devCommand": "pnpm run dev",
      "buildCommand": "pnpm run build",
      "waitTimeout": 20000
    },
    "binaryVersion": "5.6.0"
  }
}
