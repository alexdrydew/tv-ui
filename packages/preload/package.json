        "vite": "6.2.6",
        "vite-tsconfig-paths": "^5.1.4",
        "vitest": "^3.1.2"
    },
    "dependencies": {
        "@app/types": "*",
        "effect": "^3.14.11",
        "electron": "35.1.5",
        "freedesktop-icons": "^1.0.0",
        "ini": "^5.0.0"
