{
    "name": "@app/preload",
    "type": "module",
    "scripts": {
        "build": "vite build",
        "typecheck": "tsc --noEmit"
    },
    "exports": {
        ".": {
            "types": "./src/index.ts",
            "default": "./dist/_virtual_browser.mjs"
        },
        "./exposed.mjs": {
            "types": "./src/exposed.d.ts",
            "default": "./dist/exposed.mjs"
        }
    },
    "imports": {
        "#src/*" : "./src/*"
    },
    "devDependencies": {
        "@app/electron-versions": "*",
        "mlly": "1.7.4",
        "typescript": "5.8.3",
        "vite": "6.2.6",
        "vite-tsconfig-paths": "^5.1.4"
    },
    "dependencies": {
        "@app/types": "*",
        "@zod-plugin/effect": "^0.1.0",
        "effect": "^3.14.8",
        "electron": "35.1.5"
    }
}
